@model RegisterVM

<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Login</h4>

            <form method="post" class="forms-sample" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="Username" for="exampleInputUsername1">UserName</label>
                    <input asp-for="Username" class="form-control" >
                </div>
                <div class="form-group">
                    <label asp-for="Name" for="exampleInputUsername1">Name</label>
                    <input asp-for="Name" class="form-control" >
                </div>
                <div class="form-group">
                    <label asp-for="Surname" for="exampleInputUsername1">Surname</label>
                    <input asp-for="Surname" class="form-control" >
                </div>
                <div class="form-group">
                    <label asp-for="Email" for="exampleInputPassword1">Email</label>
                    <input asp-for="Email" class="form-control">
                </div>
                <div class="form-group">
                    <label asp-for="Password" for="exampleInputEmail1">Password</label>
                    <input asp-for="Password" class="form-control"  placeholder="Paswword">
                </div>
                <div class="form-group">
                    <label asp-for="ConfrimPassword" for="exampleInputEmail1">ConfrimPassword</label>
                    <input asp-for="ConfrimPassword" class="form-control" placeholder="ConfrimPaswword">
                </div>
                

                <button type="submit" class="btn btn-primary me-2">Register</button>

            </form>
        </div>
    </div>
</div>
